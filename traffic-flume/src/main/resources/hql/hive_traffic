CREATE EXTERNAL TABLE dominic.hive_traffic (
	hour INT,
	minute INT,	
	cam INT,
	car ARRAY<STRUCT<num:STRING, speed:INT>>)
PARTITIONED BY (date STRING, road STRING)
ROW FORMAT DELIMITED 
FIELDS TERMINATED BY ','
COLLECTION ITEMS TERMINATED BY '|'
MAP KEYS TERMINATED BY '^'
STORED AS SEQUENCEFILE
LOCATION '/user/ndap/repository/user/dominic/traffic';